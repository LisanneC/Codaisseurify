  <div class="col-md-9">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h1>Artists</h1>
      </div>
      
      <div class="panel-body">
        <% @artists.each do |artist| %>
        <%= image_tag photo.image rescue nil%>
        <h3><%= link_to artist.name, artist_path(artist.id) %></h3>
        <% end %>
      </div>
    </div>
  </div>
</div>
