<%= form_for [@artist,@artist.songs.new] do |f| %>

  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="form-group">
    <%= f.label :year_of_release %>
    <%= f.number_field :year_of_release %>
  </div>

  <div class="form-group">
    <%= f.submit "Add new song" %>
  </div>

<% end %>
